{
  "productName": "WinstyleS",
  "version": "0.1.0",
  "identifier": "com.thankyoucheems.winstyles",
  "build": {
    "beforeDevCommand": "python -m http.server 1420 --directory ../frontend",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../frontend"
  },
  "app": {
    "windows": [
      {
        "title": "WinstyleS",
        "width": 1100,
        "height": 720,
        "resizable": true,
        "fullscreen": false
      }
    ]
  },
  "bundle": {
    "icon": [
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}
